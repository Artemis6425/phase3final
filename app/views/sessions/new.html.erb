<%= render '/layouts/header'%>
<% if flash[:notice] %>
  <br><%= flash[:notice] %>
<% end %>
<h2>Create User</h2>

<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :name %><br>
  <%= f.label :password %>
  <%= f.password_field :password %><br>
  <%= f.submit "Create Account"%>
<% end %>
